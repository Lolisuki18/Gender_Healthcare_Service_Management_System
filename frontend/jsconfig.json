{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@/*": ["*"],
      "@components/*": ["components/*"],
      "@common/*": ["components/common/*"],
      "@layouts/*": ["layouts/*"],
      "@pages/*": ["pages/*"],
      "@assets/*": ["assets/*"],
      "@styles/*": ["styles/*"],
      "@utils/*": ["utils/*"],
      "@hooks/*": ["hooks/*"],
      "@context/*": ["context/*"],
      "@services/*": ["services/*"],
      "@redux/*": ["redux/*"]
    }
  },
  "include": ["src"]
}
